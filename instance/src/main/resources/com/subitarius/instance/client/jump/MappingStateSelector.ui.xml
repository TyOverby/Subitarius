<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="msg"
		type="com.subitarius.instance.client.jump.JumpMessages" />

	<ui:style>
		.stateSelector td {
			padding-left: 2px;
			padding-right: 2px;
		}
	</ui:style>

	<g:HorizontalPanel addStyleNames="{style.stateSelector}">
		<g:CheckBox text="{msg.showPatrolled}" checked="true"
			ui:field="patrolledBox" />
		<g:CheckBox text="{msg.showUnpatrolled}" checked="true"
			ui:field="unpatrolledBox" />
		<g:CheckBox text="{msg.showRemoved}" checked="false"
			ui:field="removedBox" />
	</g:HorizontalPanel>

</ui:UiBinder>
