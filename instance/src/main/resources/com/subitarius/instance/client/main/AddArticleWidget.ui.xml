<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="res"
		type="com.subitarius.instance.client.CommonResources" />
	<ui:with field="msg"
		type="com.subitarius.instance.client.main.MainMessages" />

	<g:HTMLPanel>
		<table>
			<tr>
				<td colspan="3">
					<g:Label addStyleNames="{res.style.heading}" text="{msg.addArticleHeading}" />
				</td>
			</tr>
			<tr>
				<td colspan="3">
					<g:Label ui:field="statusLabel" text="{msg.statusLabel}" />
				</td>
			</tr>
			<tr>
				<td>
					<g:Label text="{msg.urlLabel}" />
				</td>
				<td>
					<g:TextBox width="600px" ui:field="urlBox" />
				</td>
				<td>
					<g:Button ui:field="addButton" text="{msg.addButton}" />
				</td>
			</tr>
		</table>
	</g:HTMLPanel>

</ui:UiBinder>
