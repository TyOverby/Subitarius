<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="res" type="com.prealpha.extempdb.instance.client.CommonResources" />
	<ui:with field="msg" type="com.prealpha.extempdb.instance.client.main.MainMessages" />

	<g:VerticalPanel visible="false">
		<g:Label addStyleNames="{res.style.heading}" text="{msg.pointsHeading}" />
		<g:Grid ui:field="pointsGrid" />
	</g:VerticalPanel>

</ui:UiBinder>
